<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" >	    <xsl:template match="/">	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <links depth="1">	                <xsl:value-of select="div[2]/h2/a/@href"/>	            </links>	        </xsl:for-each>        <xsl:for-each select="//*[@id='content']/div/div[1]/div[2]/div/table/tbody/tr">            <next_link3 next_link_depth = "3">                <xsl:value-of select="td[1]/a/@href"/>            </next_link3>        </xsl:for-each>            <!--<next_link1 next_link_depth = "1">-->                <!--<xsl:value-of select="//*[@id='content']/div/div[2]/h2/span[2]/a/@href"/>-->            <!--</next_link1>-->            <next_link2 next_link_depth = "2">                <xsl:value-of select="//*[@id='content']/div/div[1]/div[1]/span/a/@href"/>            </next_link2>            <xsl:for-each select="//*[@id='subject_list']/ul/li">	            <ebook depth="1">                    <xsl:choose>                        <xsl:when test="div[2]/div[3]/div[3]/a">                                yes                        </xsl:when>                        <xsl:otherwise>                                no                        </xsl:otherwise>                    </xsl:choose>	            </ebook>	        </xsl:for-each>	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <title depth="1">                    <xsl:value-of select="div[2]/h2/a/@title"/>	            </title>	        </xsl:for-each>	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <info depth="1">	                <xsl:value-of select="normalize-space(div[2]/div[1]/text())"/>	            </info>	        </xsl:for-each>	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <type depth="1">	                <xsl:value-of select="normalize-space(//div[@id='content']/h1/text())"/>	            </type>	        </xsl:for-each>	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <star depth="1">	                <xsl:value-of select="normalize-space(div[2]/div[2]/span[2]/text())"/>	            </star>	        </xsl:for-each>	         <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <people depth="1">	                <xsl:value-of select="normalize-space(div[2]/div[2]/span[3]/text())"/>	            </people>	        </xsl:for-each>	        <xsl:for-each select="/html/body/div[3]/div[1]/div/div[1]/div/ul/li">	            <star depth="1">	                <xsl:value-of select="normalize-space(div[2]/div[2]/span[2]/text())"/>	            </star>	        </xsl:for-each>	    </xsl:template>	</xsl:stylesheet>